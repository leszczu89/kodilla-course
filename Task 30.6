DELIMITER $$

CREATE PROCEDURE INDEX_SEARCH()

BEGIN

    EXPLAIN SELECT * FROM READERS WHERE FIRSTNAME = 'John';
    CREATE INDEX READERS_FIRSTNAME ON READERS (FIRSTNAME);
    CREATE INDEX READERS_LASTNAME ON READERS (LASTNAME);
    CREATE INDEX BOOKS_TITLE ON BOOKS (TITLE);
    EXPLAIN SELECT * FROM READERS WHERE FIRSTNAME = 'John';

END $$

DELIMITER ;

CALL INDEX_SEARCH();